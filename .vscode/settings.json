{
  "editor.insertSpaces": true,
  "editor.tabSize": 2,
  "editor.codeActionsOnSave": {
    "source.fixAll": true,
    "source.organizeImports": true
  },
  "cSpell.words": [
    "falsey",
    "newdash"
  ],
  "eslint.enable": false,
  "javascript.autoClosingTags": false,
  "typescript.autoClosingTags": false
}
