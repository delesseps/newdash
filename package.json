{
  "name": "@newdash/newdash",
  "description": "yet another javascript util library",
  "version": "0.0.3",
  "license": "MIT",
  "main": "./lib",
  "unpkg": "./dist/newdash.js",
  "engines": {
    "node": ">=10.0.0"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/newdash/newdash"
  },
  "contributors": [
    {
      "name": "Lodash Contributors",
      "url": "https://github.com/lodash/lodash"
    },
    {
      "name": "Theo Sun",
      "email": "theo.sun@outlook.com",
      "url": "https://github.com/Soontao"
    }
  ],
  "scripts": {
    "test": "jest",
    "lint": "eslint",
    "coverage": "npm test -- --coverage --no-cache",
    "build": "tsc && webpack",
    "prepublishOnly": "npm run build"
  },
  "devDependencies": {
    "@types/jest": "^25.2.3",
    "@typescript-eslint/eslint-plugin": "^3.0.1",
    "@typescript-eslint/parser": "^3.0.1",
    "eslint": "^7.1.0",
    "jest": "^26.0.1",
    "terser-webpack-plugin": "^3.0.2",
    "ts-jest": "^26.0.0",
    "ts-loader": "^7.0.5",
    "typescript": "^3.9.3",
    "webpack": "^4.43.0",
    "webpack-cli": "^3.3.11"
  },
  "jest": {
    "transform": {
      ".(js|ts|tsx)": "ts-jest"
    },
    "testTimeout": 30000,
    "collectCoverageFrom": [
      "src/**/*",
      "!**/node_modules/**"
    ],
    "coveragePathIgnorePatterns": [
      "node_modules/",
      "src/test"
    ]
  }
}
