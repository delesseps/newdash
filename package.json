{
  "name": "@newdash/newdash",
  "description": "yet another javascript util library",
  "version": "5.4.0",
  "license": "MIT",
  "main": "./dist/node/index",
  "engines": {
    "node": ">=10.0.0"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/newdash/newdash"
  },
  "contributors": [
    {
      "name": "Lodash Contributors",
      "url": "https://github.com/lodash/lodash"
    },
    {
      "name": "Theo Sun",
      "email": "theo.sun@outlook.com",
      "url": "https://github.com/Soontao"
    }
  ],
  "scripts": {
    "test": "jest",
    "lint": "tsc --noEmit && eslint .",
    "coverage": "npm test -- --coverage --no-cache",
    "build": "tsc --build tsconfig.json && tsc --build tsconfig.es5.json && tsc --build tsconfig.es6.json && webpack && npm run gen:package",
    "gen:package": "node scripts/copy-package-json.js",
    "prepublishOnly": "npm run build",
    "release": "standard-version --no-verify && git push --follow-tags origin master"
  },
  "devDependencies": {
    "@types/jest": "^25.2.3",
    "@typescript-eslint/eslint-plugin": "^3.0.1",
    "@typescript-eslint/parser": "^3.0.1",
    "eslint": "^7.1.0",
    "jest": "^26.0.1",
    "standard-version": "^8.0.0",
    "terser-webpack-plugin": "^3.0.2",
    "ts-jest": "^26.0.0",
    "ts-loader": "^7.0.5",
    "typescript": "^3.9.3",
    "webpack": "^4.43.0",
    "webpack-cli": "^3.3.11"
  },
  "jest": {
    "transform": {
      ".(js|ts|tsx)": "ts-jest"
    },
    "testTimeout": 30000,
    "collectCoverageFrom": [
      "src/**/*",
      "!**/node_modules/**"
    ],
    "coveragePathIgnorePatterns": [
      "node_modules/",
      "src/test"
    ]
  },
  "keywords": [
    "javascript",
    "lodash",
    "utilities"
  ]
}
