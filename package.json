{
  "name": "newdash",
  "version": "5.0.0",
  "license": "MIT",
  "private": true,
  "main": "./lib",
  "engines": {
    "node": ">=4.0.0"
  },
  "scripts": {
    "test": "jest",
    "lint": "eslint src",
    "coverage": "npm test -- --coverage --no-cache",
    "build": "babel src --ignore src/test --out-dir lib"
  },
  "devDependencies": {
    "@babel/cli": "^7.8.4",
    "@babel/core": "^7.9.6",
    "@babel/preset-env": "^7.9.6",
    "babel-jest": "^26.0.1",
    "eslint": "^7.1.0",
    "eslint-plugin-import": "^2.20.2",
    "jest": "^26.0.1"
  },
  "jest": {
    "testTimeout": 30000,
    "collectCoverageFrom": [
      "src/**/*.js",
      "!**/node_modules/**"
    ],
    "coveragePathIgnorePatterns": [
      "node_modules/",
      "src/test"
    ]
  }
}
